import java.lang.*;
import java.util.*;
public class Tables
{
    public static void main(String[] args) 
    {
        int N,i,res;
        Scanner sc=new Scanner(System.in);
        N=sc.nextInt();
        for(i=1;i<=10;i++)
        {
            res=N*i;
            System.out.printf("%d x %d = %d\n",N,i,res);
        }
    }
    
}
